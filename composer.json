{
    "name": "battis/crud",
    "description": "Doctrine DBAL-based CRUD implementation",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "seth@battis.net"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "envms/fluentpdo": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpdocumentor/phpdocumentor": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Battis\\CRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Battis\\CRUD\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
